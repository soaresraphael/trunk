<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	<managed-bean>
		<managed-bean-name>DirectoriesView</managed-bean-name>
		<managed-bean-class>com.msprojectreport.view.DirectoriesView</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<navigation-rule>
		<from-view-id>/atividade.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>atividade</from-outcome>
			<to-view-id>/atividade.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/tela.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>tela</from-outcome>
			<to-view-id>/tela.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
