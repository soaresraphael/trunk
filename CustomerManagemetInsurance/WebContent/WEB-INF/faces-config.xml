<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	<navigation-rule>
		<from-view-id>/content/atividade.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>atividade</from-outcome>
			<to-view-id>/content/atividade.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/tela.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>tela</from-outcome>
			<to-view-id>/tela.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/content/addEditCustomers.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>addEditCustomers</from-outcome>
			<to-view-id>/customers.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/content/detailCustomer.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>detailCustomer</from-outcome>
			<to-view-id>/content/detailCustomer.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/content/basicContent.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>index</from-outcome>
			<to-view-id>/content/basicContent.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	
</faces-config>
